{
  "buildCommand": "cd savor-site && npm run build",
  "outputDirectory": "savor-site/.vercel/output/static",
  "installCommand": "cd savor-site && npm install",
  "framework": "astro",
  "env": {
    "WORDPRESS_API": "https://savorcms.wpcomstaging.com/wp-json/wp/v2"
  },
  "rewrites": [
    {
      "source": "/unbound",
      "destination": "/unbound.html"
    }
  ]
}